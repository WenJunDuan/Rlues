{
  "name": "vibecoding-kernel",
  "version": "7.8.0",
  "description": "VibeCoding Kernel - AI编程协作系统，支持多AI协调、智能工作流、知识库集成",
  "author": "Mi_Manchi",
  "homepage": "https://github.com/your-repo/vibecoding-kernel",
  "license": "MIT",
  "keywords": [
    "claude-code",
    "ai-programming",
    "multi-ai",
    "workflow",
    "knowledge-base"
  ],
  "components": {
    "skills": [
      "skills/phase-router/SKILL.md",
      "skills/knowledge-base/SKILL.md",
      "skills/experience/SKILL.md",
      "skills/service-analysis/SKILL.md",
      "skills/context7/SKILL.md",
      "skills/continuous-learning/SKILL.md",
      "skills/verification-loop/SKILL.md",
      "skills/strategic-compact/SKILL.md",
      "skills/riper/SKILL.md",
      "skills/cunzhi/SKILL.md",
      "skills/memory/SKILL.md",
      "skills/code-quality/SKILL.md",
      "skills/multi-ai/SKILL.md"
    ],
    "agents": [
      "agents/phase-router.md",
      "agents/requirement-mgr.md",
      "agents/design-mgr.md",
      "agents/impl-executor.md",
      "agents/experience-mgr.md"
    ],
    "commands": [
      "commands/vibe-init.md",
      "commands/vibe-plan.md",
      "commands/vibe-todos.md",
      "commands/vibe-review.md",
      "commands/vibe-dev.md",
      "commands/vibe-service.md",
      "commands/vibe-exp.md",
      "commands/vibe-kb.md",
      "commands/learn.md",
      "commands/checkpoint.md",
      "commands/verify.md"
    ],
    "contexts": [
      "contexts/dev.md",
      "contexts/review.md",
      "contexts/research.md"
    ]
  },
  "mcp": {
    "augment-context-engine": {
      "args": ["--mcp"],
      "command": "auggie",
      "env": {
        "AUGMENT_API_TOKEN": "",
        "AUGMENT_API_URL": ""
      },
      "type": "stdio"
    },
    "chrome-devtools": {
      "args": ["-y", "chrome-devtools-mcp@latest"],
      "command": "npx.cmd",
      "type": "stdio"
    },
    "context7": {
      "args": ["-y", "@upstash/context7-mcp", "--api-key", ""],
      "command": "npx.cmd",
      "type": "stdio"
    },
    "cunzhi": {
      "args": [],
      "command": "E:\\CodeTool\\AICode\\cunzhi-cli\\寸止.exe",
      "type": "stdio"
    },
    "desktop-commander": {
      "args": ["-y", "@wonderwhy-er/desktop-commander"],
      "command": "npx.cmd",
      "env": {
        "SYSTEMROOT": "C:\\Windows"
      },
      "type": "stdio"
    },
    "everything": {
      "args": ["-y", "@modelcontextprotocol/server-everything"],
      "command": "npx.cmd",
      "type": "stdio"
    },
    "fetch": {
      "args": ["mcp-server-fetch"],
      "command": "uvx",
      "env": {
        "PYTHONIOENCODING": "utf-8"
      },
      "type": "stdio"
    },
    "mcp-deepwiki": {
      "args": ["-y", "mcp-deepwiki@latest"],
      "command": "npx.cmd",
      "type": "stdio"
    },
    "memory": {
      "args": ["-y", "@modelcontextprotocol/server-memory"],
      "command": "npx.cmd",
      "env": {
        "MEMORY_FILE_PATH": "E:\\workspace\\AICode\\server-memory\\memory.json"
      },
      "type": "stdio"
    },
    "promptx": {
      "args": ["-y", "@promptx/mcp-server"],
      "command": "npx.cmd",
      "type": "stdio"
    },
    "sequential-thinking": {
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"],
      "command": "npx.cmd",
      "type": "stdio"
    }
  },
  "configuration": {
    "dataDirectory": ".ai_state",
    "knowledgeBaseDirectory": "knowledge-base",
    "experienceDirectory": ".ai_state/experience"
  }
}
